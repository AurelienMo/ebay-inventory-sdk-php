{
    "name": "amorvan/ebay-inventory-sdk-php",
    "description": "PHP SDK for Ebay Inventory API",
    "keywords": [
        "php",
        "sdk",
        "api"
    ],
    "license": [
        "MIT"
    ],
    "authors": [
        {
            "name": "Aurelien Morvan",
            "email": "morvan.aurelien@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "symfony/serializer": "^5.4",
        "symfony/property-access": "^5.4",
        "phpdocumentor/reflection-docblock": "^5.3",
        "symfony/validator": "^5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "squizlabs/php_codesniffer": "^3.6",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": { "SapientPro\\EbayInventorySDK\\" : "src/"}
    },
    "autoload-dev": {
        "psr-4": { "SapientPro\\EbayInventorySDK\\Tests\\" : "tests/"}
    }
}
